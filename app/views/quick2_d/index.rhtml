<a href="#" onClick="openHelpWindow('/quick2_d/help_ov');" onmouseover="return overlib('Help');" onmouseout="return nd();"><img src="/images/helpbutton.gif" border="0" style="float: right; padding-top: 2px;"></a><h2><%= @page_title %></h2>

<%= form_tag({:action => :run, :job => 'quick2_d'}, {:multipart => true})%> 

<h3 class="<%= @section["name"] %>">Input</h3>
<div class="row">
	<%= form_label("Paste protein sequence or MSA", "input") %>
	<%= form_text_area("sequence_input") %>
</div>
<div class="row">
  <div class="formw">
    <span style="color: #008C50;">
      View alignment of <a href="#" style="color:#008C50;" onClick="javascript:pasteExample();">viral DNA ligases.</a>
    </span>
  </div>
</div>
<div class="row">
	<%= form_label("or upload a local file") %>
	<%= form_file("sequence_file") %>
</div>	
<div class="row">
	<%= form_label("Select input format", "format") %>
	<%= form_select_single("informat", @informat_values, @informat_labels, @informat_values[0]) %>	
</div><div class="spacer">&nbsp;</div>

<div class="row">
	<%= form_submit_snail_buttons("Runtime ~5 min for N=100 sequences of length L=200.") %>
</div>

<div class="spacer">&nbsp;</div>

<h3 class="<%= @section["name"] %>">Options</h3>

<div style="min-height: 80px;">
    <div class="row">
         <%= form_label("MSA Generation Method", "msa_generation_label") %>
         <%= form_radio_buttons("msa_generation_method", @msa_gen_values, @msa_gen_labels, "input_only", "&nbsp;") %>
    </div>
    <div class="row" id="max_iter_row">
         <%= form_label("Maximal number of Iterations", "max_iter_label") %>
        <%= form_select_single("max_iter", @max_iter_allowed, @max_iter_allowed, @max_iter_allowed[0]) %>	
     </div>
    <div class="row" id="hhblitsdb_row">
      <%= form_label("Select HMM database", "databases") %>
        <table cellspacing="0">
        <tr align="center"><td><b>Standard</b></td></tr>
        <tr><td>
        <%= form_select_single("hhblits_dbs", @dbvalues, @dblabels, @default_db, 5, "", false, true) %>
        </td></tr>
        </table>
    </div>
</div>     
<!-- copied from hhpred -->
<!-- copy end -->
<div class="spacer">&nbsp;</div>


