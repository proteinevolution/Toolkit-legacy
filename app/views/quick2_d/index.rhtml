<script>

(function() {

  document.getElementById("max_iter_row").style.display = "none";
  document.getElementById("hhblitsdb_row").style.display = "none";

 var nodeList = document.getElementsByName("msa_generation_method");
 for (var i = 0; i < nodeList.length; ++i) {
    
    nodeList[i].onchange = function() {
    
    if(this.value == "input_only") {

        document.getElementById("max_iter_row").style.display = "none";    
        document.getElementById("hhblitsdb_row").style.display = "none";

    } else if(this.value == "psiblast") {

         document.getElementById("max_iter_row").style.display = "block"; 
        document.getElementById("hhblitsdb_row").style.display = "none";

    } else if(this.value == "hhblits") {

         document.getElementById("max_iter_row").style.display = "block"; 
         document.getElementById("hhblitsdb_row").style.display = "block";

    }
    };    
}
})(); 
</script>




