<div id="jobsection"  class="<%= @section["name"] %>">
  <ul class="<%= @section["name"] %>">
    <% for i in 0...@shortlinks.length%>
      <li <% if @shortlinks[i] == @color %> class="current" <% end %>>
        <a href="/clubsubp/<%= @links[i]%>" onmouseover="return overlib('<%= @link_text[i]%>');" onmouseout="return nd();"><%= @link_names[i]%></a>
      </li>
    <%end%>
  </ul>
</div>

