<%= render(:partial => "shared/jobspecs", :locals => {:title_extra => 'Results', :toolname => @job.tool}) %>
  
<%= render(:partial => "shared/resultsections", :locals => {:export_to => 'rejected'}) %>

<%= form_tag( "", {:id => @job.tool+"_form", :name => @job.tool+"_form"}) %>
  

<% if @job.rejected_res.length > 0 %>
	<div class="alignres"><%= @job.rejected_res.join("\n") %></div>
<% else %>
	<div class="alignres">There are no rejected sequences!</div>
<% end %>

</form>